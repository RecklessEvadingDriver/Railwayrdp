# Railway VPS Environment Configuration

# Default root password (override with Railway environment variable)
ROOT_PASSWORD=railway123

# SSH settings
SSH_PORT=22

# Performance tuning for 32GB RAM / 32 vCPU
# Adjust these if needed
export OMP_NUM_THREADS=32
export MAKEFLAGS="-j32"

# Locale settings
export LANG=en_US.UTF-8
export LC_ALL=en_US.UTF-8
